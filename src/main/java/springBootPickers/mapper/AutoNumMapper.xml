<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="springBootPickers.mapper.AutoNumMapper">
<select id="AutoNumSelect" resultType="string">
select concat(#{sep}, nvl(max(substr(${columnName}, #{len})), 100000) + 1) from ${tableName}
</select>
<delete id="numsDelete">	<!-- 파라미터 타입 여러개일땐 안적어줘도됨 -->
delete from ${tableName}
where ${columnName} in
	<foreach collection="nums" item="num" open="(" close=")" index="index"
	separator=",">
	#{num}
	</foreach>
</delete>
</mapper>